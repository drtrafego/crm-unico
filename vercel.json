{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://cliente.casaldotrafego.com https://clientes.casaldotrafego.com"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOW-FROM https://cliente.casaldotrafego.com"
                }
            ]
        },
        {
            "source": "/api/webhooks/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "true"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, PUT, DELETE, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Origin"
                }
            ]
        }
    ]
}